{
  "properties": {
    "apiVersion": "0.0.1-SNAPSHOT",
    "libraryVersion": "0.2.0-SNAPSHOT",
    "ktorVersion": "1.3.2"
  },
  "link": "https://github.com/WaveBeans/jupyter-wave",
